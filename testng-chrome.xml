<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Crome Browser Test Suite" thread-count="3" parallel="methods" data-provider-thread-count="3">
    <test name="ChromeTest" verbose="2">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.opencarttest.test.NavigationTest"/>
            <class name="com.opencarttest.test.ProductPageTest"/>
            <class name="FrameworkTest"/>
        </classes>
    </test>
</suite>

